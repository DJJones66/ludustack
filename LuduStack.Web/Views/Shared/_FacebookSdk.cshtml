@{
    Layout = null;
}

<script>
    $(document).ready(function () {
        $.ajaxSetup({ cache: true });
        $.getScript('https://connect.facebook.net/en_US/sdk.js', function () {
            FB.init({
                appId: $('meta[property="fb:app_id"]').attr('content'),
                version: 'v5.0',
                autoLogAppEvents: true,
                xfbml: true
            });
        });
    });
</script>